CREATE INDEX fk_a_state ON devel.account ( state );

CREATE INDEX fk_a_broker ON devel.account ( broker );

CREATE INDEX fk_a_environment ON devel.account ( environment );

CREATE INDEX fk_a_owner ON devel.account ( owner );

