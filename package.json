{
  "name": "blofin-data",
  "version": "1.0.0",
  "main": "./app.ts",
  "scripts": {
    "main": "npx tsx app > app.log",
    "test": "npx tsx ./test/modules/$TEST_SCRIPT",
    "import": "npx tsx import.ts > import.log",
    "cli:dev": "node --experimental-strip-types --env-file=.env.local.devel ./app/cli.ts",
    "build": "esbuild ./app.ts --bundle --platform=node --format=esm --target=node22 --outfile=./build/js/app.js --tsconfig=./tsconfig.json",
    "start:test": "node --experimental-strip-types --env-file=.env.local.devel test.ts",
    "start:dev": "node --experimental-strip-types --env-file=.env.local.devel app.ts",
    "start:prod": "node --experimental-strip-types --env-file=.env.local.prod app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "imports": {
    "#api": "./api/index.ts",
    "#api/*": "./api/*.ts",
    "#app/*": "./app/*.ts",
    "#db": "./db/index.ts",
    "#db/*": "./db/*.ts",
    "#lib/*": "./lib/*.ts",
    "#module/*": "./module/*.ts",
    "#cli/*": "./cli/*.ts",
    "#fcrt/*": "./fcrt/*.ts",
    "#stops": "./module/stops/index.ts",
    "#requests": "./module/requests/index.ts"
  },
  "dependencies": {
    "console-log-colors": "^0.5.0",
    "decimal.js": "^10.6.0",
    "mysql2": "^3.13.0",
    "nanoid": "^3.3.9",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/prompts": "^2.4.9",
    "chalk": "^5.6.2",
    "esbuild": "0.27.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djorgens2/blofin-data.git"
  },
  "bugs": {
    "url": "https://github.com/djorgens2/blofin-data/issues"
  },
  "homepage": "https://github.com/djorgens2/blofin-data#readme",
  "description": ""
}
